{
  "name": "eve-common",
  "version": "1.3.0",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "install:skipdev": "npm install --production",
    "test": "npm run check && nyc ava",
    "build": "tsc || true",
    "test:watch": "ava --watch",
    "check": "tsc --noEmit",
    "prepublish": "npm run build",
    "coverage-report": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "keywords": [],
  "author": "Expando global s.r.o.",
  "license": "MIT",
  "devDependencies": {
    "@sentry/node": "^4.5.3",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.1",
    "@types/mongoose": "^5.3.12",
    "@types/node": "^10.12.21",
    "@types/winston": "^2.4.4",
    "amazon-mws": "0.0.23",
    "ava": "^1.2.0",
    "codecov": "^3.1.0",
    "dotenv": "^6.2.0",
    "imparsable": "^1.0.7",
    "injection-js": "^2.2.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "mongoose": "^5.4.8",
    "nock": "^10.0.6",
    "nyc": "^13.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.1",
    "winston": "^3.2.1"
  },
  "ava": {
    "compileEnhancements": false,
    "files": [
      "src/**/__tests__/*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "reflect-metadata",
      "ts-node/register/transpile-only"
    ],
    "sources": [
      "src/**/*.ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "dist",
      "**/*.d.ts"
    ],
    "all": true
  },
  "dependencies": {
    "@types/got": "^9.4.0",
    "got": "^9.6.0",
    "koa-bearer-token": "^1.0.0",
    "koa-compose": "^4.1.0",
    "request-promise-native": "^1.0.5",
    "ts-essentials": "^2.0.0"
  },
  "peerDependencies": {
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "reflect-metadata": "^0.1.13",
    "amazon-mws": "0.0.23",
    "injection-js": "^2.2.1",
    "mongoose": "^5.4.8",
    "@sentry/node": "^4.5.3",
    "dotenv": "^6.2.0",
    "rxjs": "^6.4.0",
    "winston": "^3.2.1",
    "imparsable": "^1.0.7"
  }
}
