{
  "name": "eve-common",
  "version": "7.4.2",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "install:skipdev": "npm install --production",
    "test": "npm run check && nyc ava",
    "build": "tsc || true",
    "test:watch": "ava --watch",
    "ava": "ava",
    "check": "tsc --noEmit",
    "check:watch": "tsc --noEmit -w",
    "prepublish": "npm run build",
    "coverage-report": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "keywords": [],
  "author": "Expando global s.r.o.",
  "license": "MIT",
  "devDependencies": {
    "@sentry/node": "^5.10.1",
    "@types/got": "^9.6.9",
    "@types/joi": "^14.3.4",
    "@types/koa": "^2.11.0",
    "@types/koa-bodyparser": "^4.2.1",
    "@types/mongoose": "^5.5.32",
    "@types/node": "^12.12.14",
    "amazon-mws": "0.0.23",
    "ava": "^2.4.0",
    "bson": "^4.0.2",
    "codecov": "^3.6.1",
    "dotenv": "^8.2.0",
    "got": "^9.6.0",
    "imparsable": "^1.0.7",
    "injection-js": "^2.2.2",
    "joi": "^14.3.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-joi-router": "^5.2.0",
    "mongoose": "^5.7.14",
    "nock": "^11.7.0",
    "nyc": "^13.3.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "rxjs": "^6.5.3",
    "timekeeper": "^2.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3",
    "prettier": "^1.19.1"
  },
  "ava": {
    "compileEnhancements": false,
    "files": [
      "src/**/__tests__/*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "reflect-metadata",
      "ts-node/register"
    ],
    "sources": [
      "src/**/*.ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "dist",
      "**/*.d.ts"
    ],
    "all": true
  },
  "prettier": {
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "printWidth": 120
  },
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "@types/pino": "^5.14.0",
    "fast-safe-stringify": "^2.0.7",
    "koa-bearer-token": "^1.0.0",
    "koa-compose": "^4.1.0",
    "lodash": "^4.17.15",
    "pino": "^5.14.0",
    "request-promise-native": "^1.0.8",
    "ts-essentials": "^2.0.12"
  },
  "peerDependencies": {
    "got": "^9.6.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "reflect-metadata": "^0.1.13",
    "amazon-mws": "0.0.23",
    "injection-js": "^2.2.1",
    "mongoose": "^5.4.8",
    "@sentry/node": "^4.5.3",
    "koa-joi-router": "^5.2.0",
    "dotenv": "^6.2.0",
    "rxjs": "^6.4.0",
    "winston": "^3.2.1",
    "joi": "^14.3.1",
    "imparsable": "^1.0.7"
  }
}
