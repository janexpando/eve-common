{
  "name": "eve-common",
  "version": "0.1.1",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "test": "npm run check && nyc ava",
    "build": "tsc",
    "test:watch": "ava --watch",
    "check": "tsc --noEmit",
    "prepublish": "npm run build",
    "coverage-report": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "keywords": [],
  "author": "Expando global s.r.o.",
  "license": "MIT",
  "devDependencies": {
    "ava": "^1.2.0",
    "codecov": "^3.1.0",
    "nyc": "^13.1.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.1"
  },
  "ava": {
    "compileEnhancements": false,
    "files": [
      "src/**/__tests__/*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "sources": [
      "src/**/*.ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "dist",
      "**/*.d.ts"
    ],
    "all": true
  }
}
